    "@testing-library/react": "10.0.3",
    "@wordpress/api-fetch": "3.12.0",
    "@wordpress/block-editor": "3.8.0",
    "@wordpress/blocks": "6.14.1",
    "@wordpress/components": "9.3.0",
    "@wordpress/data": "4.15.0",
    "@wordpress/deprecated": "2.8.0",
    "@wordpress/dom-ready": "2.7.0",
    "@wordpress/e2e-test-utils": "4.6.0",
    "@wordpress/editor": "9.16.0",
    "@wordpress/element": "2.12.0",
    "@wordpress/env": "1.3.0",
    "@wordpress/eslint-plugin": "3.3.0",
    "@wordpress/hooks": "2.8.0",
    "@wordpress/i18n": "3.9.0",
    "@wordpress/icons": "1.3.1",
    "@wordpress/jest-puppeteer-axe": "1.8.0",
    "@wordpress/media-utils": "1.8.0",
    "@wordpress/scripts": "9.0.0",
    "eslint": "6.8.0",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-import": "2.20.2",
    "husky": "4.2.5",
    "jest-silent-reporter": "0.2.1",
    "lodash": "4.17.15",
    "puppeteer": "2.0.0",
    "react": "16.13.1",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpackbar": "4.0.0"
  },
  "scripts": {
    "build": "wp-scripts build",
    "deploy": "grunt deploy",
    "dev": "wp-scripts start",
    "env": "wp-env",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "wp-scripts lint-js js/src tests/e2e",
    "lint:js:files": "wp-scripts lint-js",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "lint:php": "vendor/bin/phpcs",
    "lint:php:fix": "vendor/bin/phpcbf",
    "test:e2e": "wp-scripts test-e2e --config=tests/e2e/jest.config.js --runInBand",
    "test:e2e:interactive": "npm run test:e2e -- --puppeteer-interactive",
    "test:js": "wp-scripts test-unit-js",
    "test:php": "composer test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./bin/pre-commit.sh"
    }
  }
}
